{
  "name": "ng-second-app-test-domain-util",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ng-second-app/test-domain/util/src",
  "prefix": "ng-second-app",
  "tags": ["app:ng-second-app", "scope:ng-second-app-test-domain", "type:util"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ng-second-app/test-domain/util/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/ng-second-app/test-domain/util/**/*.ts",
          "libs/ng-second-app/test-domain/util/**/*.html"
        ]
      }
    }
  }
}
